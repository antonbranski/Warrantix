<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
	<item android:state_checked="true">
		<shape>
			<solid android:color="@color/wx_green_color"/>
			<corners android:radius="@dimen/_99sdp"/>
			<stroke
				android:width="1px"
				android:color="@color/wx_third_color"/>
		</shape>
	</item>
	<item android:state_checked="false">
		<shape>
			<solid android:color="@color/wx_fourth_color"/>
			<corners android:radius="@dimen/_99sdp"/>
			<stroke
				android:width="1px"
				android:color="@color/wx_fourth_color"/>
		</shape>
	</item>
</selector>